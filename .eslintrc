{
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:jsx-a11y/recommended",
        "plugin:react/recommended",
        "plugin:import/typescript"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": ["react", "jsx-a11y", "import"],
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jest": true
    },
    "rules": {
        "no-console": 1
    },
    "settings": {
        "react": {
            "version": "16"
        }
    }
}
